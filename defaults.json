{
  "spider": {
    "artoo": {
      "log": {
        "welcome": false,
        "enabled": false
      }
    },
    "auth": false,
    "autoExit": true,
    "autoRetry": false,
    "globalTimeout": false,
    "limit": false,
    "concurrency": 1,
    "headers": false,
    "maxRetries": 3,
    "method": "GET",
    "phantomPage": false,
    "timeout": 5000
  },
  "spawn": {
    "autoClose": true,
    "autoRestart": true,
    "args": {
      "ignoreSslErrors": true
    }
  }
}
